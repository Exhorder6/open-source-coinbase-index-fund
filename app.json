{
  "name": "Open Source GDAX Index Fund",
  "description": "Create your own index fund on GDAX that replicates Coinbase's Index Fund.",
  "repository": "https://github.com/acmeyer/open-source-gdax-index-fund",
  "keywords": ["node", "gdax", "crypto", "coinbase", "tokens", "trading", "exchange", "javascript"],
  "formation": {
    "web": {
      "quantity": 1,
      "size": "free"
    }
  },
  "env": {
    "GDAX_FIAT_CURRENCY": {
      "description": "The fiat currency you want to use for your trades.",
      "value": "USD"
    },
    "GDAX_API_KEY": {
      "description": "Your GDAX API key.",
      "value": ""
    },
    "GDAX_API_SECRET": {
      "description": "Your GDAX API secret.",
      "value": ""
    },
    "GDAX_API_PASSPHRASE": {
      "description": "Your GDAX API passphrase.",
      "value": ""
    }
  },
  "addons": [
    "scheduler",
    {
      "plan": "standard"
    }
  ]
}